<p id="notice"><%= notice %></p>

<h1>Profile2s</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profile2s.each do |profile2| %>
      <tr>
        <td><%= link_to 'Show', profile2 %></td>
        <td><%= link_to 'Edit', edit_profile2_path(profile2) %></td>
        <td><%= link_to 'Destroy', profile2, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Profile2', new_profile2_path %>
